{
  "name": "@agenite/agent",
  "version": "0.0.0",
  "description": "Simple and maintainable agent library",
  "private": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "check-types": "tsc --noEmit",
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "@agenite/llm": "workspace:*",
    "@agenite/tool": "workspace:*"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/tsup-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^20.17.14",
    "tsup": "^8.0.0",
    "@agenite/ollama": "workspace:*",
    "@agenite/bedrock": "workspace:*",
    "typescript": "^5.7.3"

  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "CHANGELOG.md"
  ]
}
