{
  "name": "@agenite/agent",
  "version": "0.4.0",
  "description": "Simple and maintainable agent library",
  "private": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "check-types": "tsc --noEmit",
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "@agenite/llm": "workspace:*",
    "@agenite/tool": "workspace:*"
  },
  "devDependencies": {
    "@agenite/bedrock": "workspace:*",
    "@agenite/ollama": "workspace:*",
    "@repo/eslint-config": "workspace:*",
    "@repo/tsup-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^20.17.25",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "CHANGELOG.md"
  ],
  "keywords": [
    "ai",
    "generative-ai",
    "agent",
    "llm",
    "chatbot",
    "typescript",
    "multi-agent",
    "tool-integration",
    "openai",
    "anthropic",
    "bedrock",
    "ollama",
    "streaming",
    "ioc"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/subeshb1/agenite.git",
    "directory": "packages/agent"
  },
  "homepage": "https://github.com/subeshb1/agenite/tree/main/packages/agent#readme",
  "bugs": {
    "url": "https://github.com/subeshb1/agenite/issues"
  },
  "license": "MIT"
}
