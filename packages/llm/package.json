{
  "name": "@agenite/llm",
  "version": "0.2.1",
  "description": "LLM interface for Agenite",
  "private": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
